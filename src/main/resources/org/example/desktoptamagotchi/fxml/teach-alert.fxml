<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.desktoptamagotchi.controllers.TeachController">

    <Label fx:id="label"/>

    <HBox alignment="CENTER" spacing="15.0">
        <TextField fx:id="textField" onAction="#onSubmit"/>
        <Button onAction="#onSubmit" text="Submit"/>
    </HBox>
</VBox>
